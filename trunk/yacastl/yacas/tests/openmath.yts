
NextTest("Converting to and from OpenMath expressions");

Macro(OMTest1,{expr}) 
[
  Local(string,result);
  string:=ToString() OMForm(@expr);
  result:=FromString(string)OMRead();
//  Echo(Hold(@expr),`Hold(@result));
  Verify(Hold(@expr),`Hold(@result));
];

OMTest1(2+3);
OMTest1(2*a+3*Sin(Cos(a*x+b)));

